{
	"$schema": "https://turborepo.org/schema.json",
	"tasks": {
		"build": {
			"dependsOn": ["^build"],
			"outputs": ["dist/**", ".next/**"],
			"env": [
				"SANITY_STUDIO_PREVIEW_ORIGIN",
				"NEXT_PUBLIC_APP_URL",
				"NEXT_PUBLIC_SANITY_STUDIO_URL",
				"NEXT_PUBLIC_SANITY_PROJECT_ID",
				"NEXT_PUBLIC_SANITY_DATASET",
				"SANITY_VIEWER_TOKEN"
			]
		},
		"lint": {
			"outputs": []
		},
		"typecheck": {
			"dependsOn": ["^typecheck"],
			"cache": false
		},
		"dev": {
			"cache": false
		}
	}
}
